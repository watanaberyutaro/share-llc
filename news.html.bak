        /* Page Hero - using recruit-hero style */

        /* News List */
        .news-list-section {
            padding: 80px 0;
            max-width: 1000px;
            margin: 0 auto;
        }

        .news-list {
            display: flex;
            flex-direction: column;
            gap: 30px;
        }

        .news-card {
            background: white;
            border: 1px solid #252525;
            overflow: hidden;
            transition: border-color 0.3s ease;
            display: flex;
            flex-direction: row;
        }

        .news-card:hover {
            border-color: #636363;
        }

        .news-card-image {
            width: 280px;
            flex-shrink: 0;
            overflow: hidden;
            background: #f8f8f8;
            padding: 30px;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .news-card-image img {
            width: 100%;
            height: 100%;
            object-fit: contain;
        }

        .news-card-content {
            padding: 30px;
            flex: 1;
            display: flex;
            flex-direction: column;
        }

        .news-meta {
            display: flex;
            align-items: center;
            gap: 15px;
            margin-bottom: 15px;
        }

        .news-meta time {
            font-size: 14px;
            font-weight: 700;
            color: #999;
        }

        .news-badge {
            padding: 4px 12px;
            background: #e74c3c;
            color: white;
            font-size: 12px;
            font-weight: 700;
            border-radius: 4px;
        }

        .news-category {
            padding: 4px 12px;
            background: #f0f0f0;
            color: #666;
            font-size: 14px;
            border-radius: 4px;
        }

        .news-card h3 {
            font-size: 20px;
            font-weight: 700;
            color: #333;
            margin-bottom: 10px;
            line-height: 1.6;
        }

        .news-card p {
            font-size: 15px;
            color: #666;
            line-height: 1.8;
            margin: 0;
        }

        /* Pagination */
        .pagination {
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 10px;
            margin-top: 60px;
        }

        .pagination button {
            padding: 12px 24px;
            background: white;
            border: 2px solid #e0e0e0;
            color: #666;
            text-decoration: none;
            border-radius: 6px;
            font-weight: 700;
            transition: all 0.3s ease;
            cursor: pointer;
            font-size: 14px;
        }

        .pagination button:hover:not(:disabled) {
            border-color: #333;
            color: #333;
        }

        .pagination button:disabled {
            opacity: 0.3;
            cursor: not-allowed;
        }

        .pagination span {
            font-size: 14px;
            color: #666;
            font-weight: 700;
        }

        @media (max-width: 768px) {
            .page-hero h1 {
                font-size: 32px;
            }

            .news-card {
                flex-direction: column;
            }

            .news-card-image {
                width: 100%;
                height: 220px;
            }

            .news-card-content {
                padding: 20px;
            }

            .news-list-section {
                padding: 60px 20px;
            }

            .news-card {
                padding: 20px;
            }

            .news-card h3 {
                font-size: 18px;
            }

            .pagination {
                flex-wrap: wrap;
            }
        }
    </style>
